{
  "name": "man-hours-manager",
  "version": "1.0.0",
  "description": "man hour management tool.",
  "scripts": {
    "start": "concurrently \"npm run watch:tsc\" \"npm run watch:scss\" \"lite-server\" ",
    "start-heroku": "concurrently \"http-server\" ",
    "watch:tsc": "tsc -w",
    "watch:scss": "node-sass src/scss --output styles -w",
    "tsc": "tsc",
    "scss": "node-sass src/scss --output styles",
    "e2e": "concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.3.0",
    "@swimlane/ngx-datatable": "^4.1.0",
    "angular-2-local-storage": "^1.0.0",
    "angular2-materialize": "^6.3.0",
    "core-js": "^2.4.1",
    "http-status-codes": "^1.1.6",
    "materialize-css": "^0.97.8",
    "moment": "^2.17.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/http-status-codes": "^1.0.29",
    "@types/jasmine": "^2.5.36",
    "@types/materialize-css": "*",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "protractor": "~4.0.13",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "~2.1.4"
  },
  "repository": {}
}
